                android:layout_toLeftOf="@id/social_2"
                android:layout_below="@id/social_4"
                android:gravity="center"
                android:textColor="#000000"
                android:background="@drawable/add_keyword_btn_un_keyword"/>

            <TextView
                android:id="@+id/social_9"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="20dp"
                android:layout_toRightOf="@id/social_2"
                android:layout_below="@id/social_6"
                android:text="test"
                android:gravity="center"
                android:textColor="#000000"
                android:background="@drawable/add_keyword_btn_un_keyword"/>

            <TextView
                android:id="@+id/social_10"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_marginTop="20dp"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/social_7"
                android:text="test"
                android:gravity="center"
                android:textColor="#000000"
                android:background="@drawable/add_keyword_btn_un_keyword"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/social_11"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="20dp"
                android:layout_toLeftOf="@id/social_10"
                android:layout_below="@id/social_7"
                android:text="test"
                android:gravity="center"
                android:textColor="#000000"
                android:background="@drawable/add_keyword_btn_un_keyword"/>


        </RelativeLayout>


        <LinearLayout
